SELECT TS.DESCRIPCION||', '||SAL.DESCRIPCION AS "SISTEMA SALUD"
FROM TIPO_SALUD TS
INNER JOIN SALUD SAL
ON TS.TIPO_SAL_ID = SAL.TIPO_SAL_ID


SELECT AVG(COUNT(ATE.ATE_ID))
FROM ATENCION ATE
WHERE TO_CHAR(ATE.FECHA_ATENCION, 'MM')= TO_CHAR(CURRENT_DATE, 'MM')
AND TO_CHAR(ATE.FECHA_ATENCION,'YYYY')= TO_CHAR(CURRENT_DATE, 'YYYY')
GROUP BY ATE.FECHA_ATENCION;










SELECT TO_CHAR(PAC_RUN, '99G999G999' ) ||'-'|| DV_RUN AS "RUT PACIENTE",
PNOMBRE||' '|| SNOMBRE||' '|| APATERNO||' '|| AMATERNO as PACIENTE,
ROUND(MONTHS_BETWEEN(SYSDATE,FECHA_NACIMIENTO) ) as AÃ‘OS
FROM PACIENTE;
